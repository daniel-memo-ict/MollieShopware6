{
  "bootstrap": "./phpunit.autoload.php",
  "minMsi": 15,
  "minCoveredMsi": 60,
  "source": {
    "directories": [
      "src"
    ],
    "excludes": [
      "Resources",
      "Compatibility/Storefront/Route/PaymentMethodRoute/Voucher",
      "Compatibility/Storefront/Route/PaymentMethodRoute/MollieLimits",
      "Compatibility/Storefront/Route/PaymentMethodRoute/Subscription"
